    "import pandas as pd\n",
    "\n",
    "daily_df = pd.read_csv(\"data/daily_report.csv\")\n",
    "\n",
    "daily_df = (\n",
    "    daily_df[[\"Country_Region\", \"Confirmed\", \"Deaths\", \"Recovered\"]]\n",
    ")\n",
    "daily_df = daily_df.groupby(\"Country_Region\").sum().reset_index()\n",
    "daily_df"
